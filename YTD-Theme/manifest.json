{
  "name": "Youtube Theme",
  "description": "Change the current page color",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_popup": "index.html"
  },
  "manifest_version": 2
}
